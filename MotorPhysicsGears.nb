  RowBox[{"ssFinalValueTheorem", "[", 
   RowBox[{"model_", ",", " ", "inputs_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"s", 
      RowBox[{"(*", 
       RowBox[{"=", " ", 
        RowBox[{"Unique", "[", "]"}]}], "*)"}], ",", " ", "t", " ", 
      RowBox[{"(*", 
       RowBox[{"=", " ", 
        RowBox[{"Unique", "[", "]"}]}], "*)"}], ",", " ", "xformedInputs", 
      ",", " ", "expr"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"xformedInputs", " ", "=", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"LaplaceTransform", "[", 
         RowBox[{
          RowBox[{"#", "[", "t", "]"}], ",", " ", "t", ",", " ", "s"}], "]"}],
         " ", "&"}], "/@", " ", "inputs"}]}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"expr", " ", "=", " ", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"model", "[", "s", "]"}], " ", ".", "xformedInputs"}], ")"}],
        "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "Limit", " ", "is", " ", "really", " ", "slow", " ", "if", " ", "we", 
        " ", "include", " ", "assumptions", " ", 
        RowBox[{"(", 
         RowBox[{"!", "?"}], ")"}]}], ",", " ", 
       RowBox[{"so", " ", 
        RowBox[{"don", "'"}], "t"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Block", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"$Assumptions", "=", 
         RowBox[{"{", "}"}]}], "}"}], ",", " ", 
       RowBox[{
        RowBox[{"Limit", "[", 
         RowBox[{
          RowBox[{"s", " ", "expr"}], ",", " ", 
          RowBox[{"s", " ", "\[Rule]", " ", "0"}]}], "]"}], " ", "//", " ", 
        "FullSimplify"}]}], "]"}]}]}], "\[IndentingNewLine]", "]"}], 
  " "}]}], "Input",ExpressionUUID->"6390bb3f-b7c5-41b8-8c19-384e6427f962"],

