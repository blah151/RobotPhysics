       RowBox[{"Quantity", "[", "\"\<Radians\>\"", "]"}]}]}], "}"}], ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "gravity", " ", "exerts", " ", "a", " ", "constant", " ", "downard", " ",
       "force", " ", "by", " ", "acting", " ", "on", " ", "the", " ", 
      "mass"}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"gravityAcceleration", " ", "=", " ", 
      RowBox[{
      "Quantity", "[", "\"\<StandardAccelerationOfGravity\>\"", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"gravityForce", " ", "=", "  ", 
      RowBox[{"gravityAcceleration", " ", "*", " ", "mass"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{"angular", " ", "accelearation"}], ",", " ", 
       RowBox[{
       "converted", " ", "to", " ", "tangential", " ", "acceration", " ", 
        "also", " ", "acts", " ", "on", " ", "the", " ", "mass", " ", "to", 
        " ", "create", " ", "an", " ", "upward", " ", "force"}]}], " ", 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"angularAcceleration", " ", "=", " ", 
      RowBox[{"Quantity", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"\[Theta]", "''"}], "[", "t", "]"}], ",", " ", 
        "siAngularAccelerationUnits"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"tangentialAcceleration", " ", "=", " ", 
      RowBox[{"angularAcceleration", " ", "*", " ", 
       RowBox[{"radius", " ", "/", " ", "radians"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"tangentialForce", " ", "=", " ", 
      RowBox[{"tangentialAcceleration", " ", "*", " ", "mass"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"the", " ", "net", " ", "force", " ", 
